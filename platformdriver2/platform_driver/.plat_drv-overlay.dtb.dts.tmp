/dts-v1/;
/plugin/;

/ {
  compatible = "brcm,bcm2835", "brcm,bcm2708";

  fragment@0 {

    target-path = "/";
    __overlay__ {

      plat_drv: plat_drv@0 {

        compatible = "ase, plat_drv";

        gpios = <&gpio 20 1>, <&gpio 21 1>;

        status = "okay";
      };
    };
  };
};
